class Solution {
public:
    int minimumOperations(vector<int>& nums) {
        vector<int>a1(101,0);
        int ans=0;
        for(int i=0; i<nums.size();i++){
            a1[nums[i]]=1;
        }
        for(int i =1; i<101; i++){
            ans = ans+a1[i];
        }
        return ans;
        
    }
};
